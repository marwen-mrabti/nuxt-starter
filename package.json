{
  "name": "nuxt-starter",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "concurrently 'pnpm run dev:db' 'nuxt dev'",
    "dev:db": "turso dev --db-file dev.db",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@libsql/client": "^0.15.10",
    "@nuxt/eslint": "1.7.1",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "1.15.0",
    "@nuxt/image": "1.10.0",
    "@nuxtjs/color-mode": "3.5.2",
    "@nuxtjs/seo": "3.1.0",
    "@pinia/nuxt": "0.11.2",
    "@sentry/nuxt": "^9",
    "@tailwindcss/vite": "^4.1.11",
    "better-auth": "^1.3.4",
    "drizzle-orm": "^0.44.3",
    "drizzle-zod": "^0.8.2",
    "import-in-the-middle": "^1.14.2",
    "nuxt": "^4.0.1",
    "nuxt-api-shield": "0.8.2",
    "nuxt-csurf": "1.6.5",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@iconify-json/tabler": "^1.2.20",
    "@types/node": "^24.1.0",
    "concurrently": "^9.2.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.32.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-oxlint": "^1.8.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "overrides": {
      "@vercel/nft": "^0.27.4"
    },
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild"
    ]
  },
  "lint-staged": {
    "*": "pnpm lint"
  }
}
